{
  "name": "doacin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "server": "nodemon server.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "cross-env DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/doacin_test vitest run --config vitest.config.integration.js",
    "test:watch": "cross-env DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/doacin_test vitest --config vitest.config.integration.js",
    "test:ui": "cross-env DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/doacin_test vitest --ui --config vitest.config.integration.js",
    "test:coverage": "cross-env DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/doacin_test vitest run --coverage --config vitest.config.integration.js",
    "test:setup": "powershell -ExecutionPolicy Bypass -File ./scripts/run-tests.ps1",
    "test:cleanup": "powershell -ExecutionPolicy Bypass -File ./scripts/cleanup-tests.ps1"
  },
  "dependencies": {
